*.bash          text eol=lf
*.css           text eol=lf diff=css
*.df            text eol=lf 
*.htm           text eol=lf  diff=html
*.html          text eol=lf  diff=html
*.java          text eol=lf  diff=java
*.kt            text eol=lf  diff=java
*.js            text eol=lf
*.json          text eol=lf
*.jsp           text eol=lf
*.jspf          text eol=lf
*.jspx          text eol=lf
*.properties    text eol=lf
*.tld           text eol=lf
*.txt           text eol=lf
*.tag           text eol=lf
*.tagx          text eol=lf
*.xml           text eol=lf
*.yml           text eol=lf
*.csv           text eol=lf
*.tsv           text eol=lf

*.md            text eol=lf

# These files are binary and should be left untouched
# (binary is a macro for -text -diff)
*.class         binary
*.dll           binary
*.ear           binary
*.gif           binary
*.ico           binary
*.jar           binary
*.jpg           binary
*.jpeg          binary
*.png           binary
*.webp          binary
*.so            binary
*.war           binary
*.jar           binary

*.doc           binary diff=astextplain
*.DOC           binary diff=astextplain
*.docx          binary diff=astextplain
*.DOCX          binary diff=astextplain
*.dot           binary diff=astextplain
*.DOT           binary diff=astextplain
*.pdf           binary diff=astextplain
*.PDF           binary diff=astextplain
*.rtf           binary diff=astextplain
*.RTF           binary diff=astextplain
